#!/bin/sh

# Maven wrapper script

MAVEN_VERSION=3.8.6
MVN_CMD="mvn"

if [ -n "$MAVEN_OPTS" ]; then
  MVN_CMD="$MVN_CMD $MAVEN_OPTS"
fi

if [ -n "$MAVEN_HOME" ]; then
  MVN_CMD="$MAVEN_HOME/bin/mvn"
fi

if [ -n "$JAVA_HOME" ]; then
  JAVA_CMD="$JAVA_HOME/bin/java"
else
  JAVA_CMD="java"
fi

if [ -z "$MAVEN_OPTS" ]; then
  MAVEN_OPTS="-Xmx1024m"
fi

exec "$JAVA_CMD" $MAVEN_OPTS -cp "$CLASSPATH" org.apache.maven.wrapper.MavenWrapperMain "$@"